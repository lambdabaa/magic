<script src="http://static.starcitygames.com/www/images/banners/media/organizedplay/statechamp/js/jquery.csv-0.71.min.js"></script>
<script>
    var csvInput =  $('#region-csv').val();
    var regionArray = $.csv.toArrays(csvInput);

    function setRegionInfo(regionCode){
        $('#basic-info').hide();
    //find the clicked star
        var regionNumber = 0;
        for (var i=0; i<regionArray.length; i++){
            if (regionArray[i][0]===regionCode){
                regionNumber = i;
            }
        }

    //set info in info div
        $('#rc-store-name').text(regionArray[regionNumber][1]);
        $('#rc-city-name').text(regionArray[regionNumber][5]); //5
        var street = regionArray[regionNumber][2];
        if((street.search("<span"))>-1){
            street = street.substring(0,street.search("<span"));
        }
        if((street.search("<br>"))>-1){
            street = street.replace("<br>", " ");
        }
        if((street.search("#"))>-1){
            street = street.substring(0,street.search("#"));
        }
        if((street.search("/"))>-1){
            street = street.substring(0,street.search("/"));
        }
        if((street.search("&"))>-1){
            street = street.substring(0,street.search("&"));
        }
        $('#rc-address').html(regionArray[regionNumber][2]+"<br>"+regionArray[regionNumber][4]);

        if (regionArray[regionNumber][2]==="TBA"){
            $('#gmapframe').hide();
        }else{
           $('#gmapframe').show(); 
        };
        $('#gmapframe').attr('src', "https://maps.google.com/maps?q="+street+" "+regionArray[regionNumber][4]+"&z=6&iwloc=&output=embed");

        if (regionArray[regionNumber][8] != ''){
            $('#rc-reg-time').show(); 
        }else{
            $('#rc-reg-time').hide();
        };
        if (regionArray[regionNumber][9] != ''){
            $('#rc-start-time').show(); 
        }else{
            $('#rc-start-time').hide();
        };
        $('#rc-reg').text(regionArray[regionNumber][8]);
        $('#rc-start').text(regionArray[regionNumber][9]);

        console.log("'" + regionArray[regionNumber][12] + "'");
        if (regionArray[regionNumber][12] != ''){
            $('#rc-prereg-info').show(); 
        }else{
            $('#rc-prereg-info').hide();
        };
        if (regionArray[regionNumber][13] != ''){
            $('#rc-visit-fb').show(); 
        }else{
            $('#rc-visit-fb').hide();
        };
        $('#rc-fb').attr("href",regionArray[regionNumber][13]);
        $('#rc-prereg-link').attr("href",regionArray[regionNumber][12]);
        $('#rc-store-name-call').text(regionArray[regionNumber][1]);
        $('#rc-phone').text(regionArray[regionNumber][6]);
        
        if (regionArray[regionNumber][10] != ''){
            $('#rc-side-event-info').show(); 
        }else{
            $('#rc-side-event-info').hide();
        };
        $('#rc-side-event').text(regionArray[regionNumber][10]);
        
        if (regionArray[regionNumber][14] != ''){
            $('#rc-side-event-link').show(); 
        }else{
            $('#rc-side-event-link').hide();
        };
        $('#rc-side-event-link').attr("href",regionArray[regionNumber][14]);
        
        if (regionArray[regionNumber][16] != ''){
            $('#rc-side-event-info2').show(); 
        }else{
            $('#rc-side-event-info2').hide();
        };
        $('#rc-side-event2').text(regionArray[regionNumber][16]);
        
        if (regionArray[regionNumber][17] != ''){
            $('#rc-side-event-link2').show(); 
        }else{
            $('#rc-side-event-link2').hide();
        };
        $('#rc-side-event-link2').attr("href",regionArray[regionNumber][17]);
        
        if (regionArray[regionNumber][11] != ''){
            $('#rc-sunday-event-info').show(); 
        }else{
            $('#rc-sunday-event-info').hide();
        };
        $('#rc-sunday-event').text(regionArray[regionNumber][11]);
        
        if (regionArray[regionNumber][15] != ''){
            $('#rc-sunday-event-link').show(); 
        }else{
            $('#rc-sunday-event-link').hide();
        };
        $('#rc-sunday-event-link').attr("href",regionArray[regionNumber][15]);
        
        if (regionArray[regionNumber][18] != ''){
            regionArray[regionNumber][18] = regionArray[regionNumber][18].replace(/\/\//g,"/");
            $('#rc-even-more-info').html(regionArray[regionNumber][18]);
            $('#rc-even-more-info').show();
        }else{
            $('#rc-even-more-info').html(" ");
            $('#rc-even-more-info').hide();
        }
        
        $('#info').show();
    }

    $(document).ready(function() {
        /*setRegionInfo("north-east");
        $('.star-click').click(function(){
            setRegionInfo($(this).attr('id'));
        });*/
        $('#map-holder div').click(function(){
            setRegionInfo($(this).attr('id'));
        });

    });
</script></p>

