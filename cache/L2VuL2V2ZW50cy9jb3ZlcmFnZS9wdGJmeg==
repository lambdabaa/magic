 <h2><span>Top 8 Bracket</span></h2>
  <!-- BEGIN :: TOP BRACKET SLIDER -->
  <div class="top-bracket-slider improved">
    <div class="wrap-bracket-slider">

            <div class="bracket quarterfinals">
        <h3>Quarterfinals</h3>
        <div class="finalists">

                    <div class="dual-players">
                        <div class="player">
                            <p>
                                  <a href="http://magic.wizards.com/en/events/coverage/ptbfz/top-8-profiles-2015-10-17#turtenwald">1 (5) Owen Turtenwald</a>                              </p>
