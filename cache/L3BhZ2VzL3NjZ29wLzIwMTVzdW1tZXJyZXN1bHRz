<script>
var csvInput =  $('#state-csv').val();
var stateArray = $.csv.toArrays(csvInput);

function setRegionInfo(stateCode){
    var stateNumber = 0;
    for (var i=0; i<stateArray.length; i++){
        //console.log(stateArray[i][0]+" "+i);
        if (stateArray[i][0]===stateCode){
            stateNumber = i;
        }
    }
//    $('#sc-map-box').usmap({
//        stateSpecificStyles: {
//            stateCode: {fill: 'yellow'}
//        } 
//    });

    url="http://www.starcitygames.com/pages/scgop/statesmap";
    if(stateArray[stateNumber][1]!==""){
        url=stateArray[stateNumber][1];
    };
    
    window.open(url);
}

$(document).ready(function() {
    $('.star-click').click(function(){
            setRegionInfo($(this).attr('id'));
        });    
});
</script>
       
