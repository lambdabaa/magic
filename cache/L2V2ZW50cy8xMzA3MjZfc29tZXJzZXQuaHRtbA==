        <script>
            $(document).ready(function(){
                
                $('#op-menu ul.main-menu > li').mouseenter(function(){
                    var menuId = "#menu-" + $(this).attr('id').substring(5); 
                    $(menuId).show();
                });
                
                $('#op-menu ul.main-menu > li').each( function() {                   
                    $(this).mouseleave(function(){
                        var menuId = "#menu-" + $(this).attr('id').substring(5);
                        $(menuId).hide();
                    });
                });
                
            });
        </script>
        
