            <script type="text/javascript">
            
                $(document).ready( function () {

                    $.fn.fitToWidth = function(){
                    
                        var $span = $(document.createElement("span"))
                          .css("display","inline")
                          .css("font","inherit")
                          .css("white-space","inherit");

                        $(this).wrapInner($span).each(function(){
                            var $t = $(this);
                            var $s = $t.children("span");
                            var tw = $t.width();
                            var tfs = parseFloat($t.css("font-size"));

                            while($s.outerWidth() > tw) {
                                $t.css("font-size",--tfs);
                            }

                            $t.html($s.html());

                        });

                        return this;
                    }
                                        
                    $("#tabs").tabs({
                        show: function( event, ui ) {
                            $(ui.panel).find(".top8-column-row").each( function () {
                                $(this).fitToWidth();
                            })
                        },
                    });
                              
                    $(".coverage_banner_event_location").fitToWidth();
                    
                });
            
                function reloadTabs(){
                    
                    $.ajax({
                        type: 'POST',
                        url: '/scglive/ajax_event_reload.php',
                        data: 'action=reload&ajaxid=1078',
                        success: function(data) {
                            $("#updates").html( data );
                            $(".top8-column-row").each( function () { 
                                $(this).fitToWidth(); 
                            });
                        }
                    });
                }
                
                
