ass":3:{s:4:"type";s:4:"land";s:10:"deck_count";i:0;s:9:"card_meta";O:8:"stdClass":2:{s:5:"title";s:10:"Bojuka Bog";s:22:"field_card_image_front";a:0:{}}}i:23;O:8:"stdClass":3:{s:4:"type";s:7:"sorcery";s:10:"deck_count";i:0;s:9:"card_meta";O:8:"stdClass":2:{s:5:"title";s:15:"Crumble to Dust";s:22:"field_card_image_front";a:0:{}}}i:24;O:8:"stdClass":3:{s:4:"type";s:7:"instant";s:10:"deck_count";i:0;s:9:"card_meta";O:8:"stdClass":2:{s:5:"title";s:12:"Sudden Shock";s:22:"field_card_image_front";a:0:{}}}i:25;O:8:"stdClass":3:{s:4:"type";s:4:"land";s:10:"deck_count";i:0;s:9:"card_meta";O:8:"stdClass":2:{s:5:"title";s:13:"Ghost Quarter";s:22:"field_card_image_front";a:0:{}}}}          </div>
