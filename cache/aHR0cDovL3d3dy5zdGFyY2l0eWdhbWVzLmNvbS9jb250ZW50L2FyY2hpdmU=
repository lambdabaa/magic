.social-holder a:link,
.social-holder a:visited{
    text-decoration: none;
    color: #125687;
}
.social-holder a:hover{
    color: #738FA3;
}
.event-social{
    padding-left: 18px;
}
.event-social:first-child{
    padding-left: 0;
}
.event-social img {
    margin-right: 5px;
    vertical-align: middle;
    margin-bottom: 6px;
}

