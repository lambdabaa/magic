<div>	<script>
            $(document).ready(function(){
                $('.archive-year').hide();
                $('.scgop-archivetoggle > a').click(function(){
                    var isShown = $(this).hasClass('shown');
                    if (isShown){
                        $(this).removeClass('shown');
                        $(this).find('.show').html('Show');
                        $(this).find('.tri-icon').attr('src', 'http://static.starcitygames.com/www/images/banners/media/organizedplay/tri-up.png');
                        $(this).parent().next().slideUp();                        
                    }else{
                        $(this).addClass('shown');
                        $(this).find('.show').html('Hide');
                        $(this).find('.tri-icon').attr('src', 'http://static.starcitygames.com/www/images/banners/media/organizedplay/tri-down.png');
                        $(this).parent().next().slideDown();
                    }
                    
                    return false;
                });
            });
        </script>
        
