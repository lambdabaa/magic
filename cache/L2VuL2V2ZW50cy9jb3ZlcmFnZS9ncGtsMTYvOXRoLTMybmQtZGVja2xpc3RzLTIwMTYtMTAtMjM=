ont";a:0:{}}}i:22;O:8:"stdClass":3:{s:4:"type";s:8:"creature";s:10:"deck_count";i:0;s:9:"card_meta";O:8:"stdClass":2:{s:5:"title";s:15:"Selfless Spirit";s:22:"field_card_image_front";a:0:{}}}i:23;O:8:"stdClass":3:{s:4:"type";s:7:"sorcery";s:10:"deck_count";i:0;s:9:"card_meta";O:8:"stdClass":2:{s:5:"title";s:11:"Fragmentize";s:22:"field_card_image_front";a:0:{}}}}          </div>
